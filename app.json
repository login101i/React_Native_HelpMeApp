{
	"expo": {
		"name": "React_Native_Restaurant",
		"slug": "React_Native_Restaurant",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./src/assets/images/ethereum.png",
		"splash": {
			"image": "./src/assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/splash.png",
				"backgroundColor": "#dbdbdb"
			}
		},
		"web": {
			"favicon": "./src/assets/images/ethereum.png"
		}
	}
}
